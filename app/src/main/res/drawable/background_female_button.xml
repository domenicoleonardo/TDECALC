<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Item para o botÃ£o Female -->
    <item android:state_checked="true" android:id="@+id/female_checked">
        <shape android:shape="rectangle">
            <solid android:color="@color/bluecalcinha"/>
            <corners android:radius="0dp"/>
            <stroke android:color="@android:color/transparent" android:width="0dp"/>
        </shape>
    </item>
    <item android:state_checked="false" android:id="@+id/female_unchecked">
        <shape android:shape="rectangle">
            <solid android:color="@color/bluecalcinha"/>
            <corners android:radius="0dp"/>
            <stroke android:color="@android:color/transparent" android:width="0dp"/>
        </shape>
    </item>

</selector>
